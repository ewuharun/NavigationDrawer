<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ProfileFragment">


        <RelativeLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="match_parent">

            <pl.droidsonroids.gif.GifImageView
                android:layout_width="match_parent"
                android:layout_height="176dp"
                android:id="@+id/dashboardImaveView"
                android:fitsSystemWindows="true"
                android:theme="@style/ThemeOverlay.AppCompat"
                android:background="@drawable/dashboard">


            </pl.droidsonroids.gif.GifImageView>

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="150dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_height="match_parent">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:id="@+id/firstCard"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">



                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"

                                        android:orientation="vertical"
                                        android:id="@+id/houseLayout"
                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/house">

                                        </ImageView>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="0dp"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            android:text="House"
                                            app:fontFamily="@font/architects_daughter"
                                            android:textColor="#000000"
                                            android:textSize="25sp">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:id="@+id/flatLayout"
                                        android:orientation="vertical"
                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/flat">

                                        </ImageView>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="0dp"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            android:text="Flat"
                                            android:textColor="#000000"
                                            android:textSize="25sp"
                                            app:fontFamily="@font/architects_daughter">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                            </LinearLayout>

                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:id="@+id/firstCard"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">



                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:id="@+id/renterLayout"
                                        android:orientation="vertical"
                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/people">

                                        </ImageView>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="0dp"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            android:includeFontPadding="false"
                                            android:text="Tanent"
                                            android:textColor="#000000"
                                            android:textSize="25sp"
                                            app:fontFamily="@font/architects_daughter">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:id="@+id/complainLayout"
                                        android:orientation="vertical"
                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/complain">

                                        </ImageView>
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:textSize="25sp"
                                            android:textColor="#000000"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            android:layout_height="0dp"
                                            app:fontFamily="@font/architects_daughter"
                                            android:text="Complain">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                            </LinearLayout>

                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:id="@+id/firstCard"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">



                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:id="@+id/paymentDueLayout"
                                        android:orientation="vertical"
                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/duepayment">

                                        </ImageView>
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:textSize="25sp"
                                            android:textColor="#000000"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            app:fontFamily="@font/architects_daughter"
                                            android:layout_height="0dp"
                                            android:text="Payment Due">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:id="@+id/paymentClearLayout"
                                        android:orientation="vertical"

                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/clearpayment">

                                        </ImageView>
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:textSize="25sp"
                                            android:textColor="#000000"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            android:layout_height="0dp"
                                            app:fontFamily="@font/architects_daughter"
                                            android:text="Payment Done">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                            </LinearLayout>

                        </LinearLayout>



                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:id="@+id/firstCard"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">



                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:id="@+id/fileLayout"
                                        android:orientation="vertical"
                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/file">

                                        </ImageView>
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:textSize="25sp"
                                            android:textColor="#000000"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            android:layout_height="0dp"
                                            app:fontFamily="@font/architects_daughter"
                                            android:text="File">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                                <androidx.cardview.widget.CardView
                                    android:layout_width="0dp"
                                    android:layout_weight=".5"
                                    app:cardCornerRadius="5dp"
                                    android:layout_margin="2dp"
                                    android:layout_height="140dp">
                                    <LinearLayout
                                        android:layout_width="match_parent"

                                        android:orientation="vertical"
                                        android:id="@+id/emergencyLayout"
                                        android:layout_gravity="center"
                                        android:layout_height="match_parent">
                                        <ImageView
                                            android:layout_width="match_parent"
                                            android:layout_height="50dp"
                                            android:layout_weight=".5"
                                            android:src="@drawable/emergency">

                                        </ImageView>
                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:textSize="25sp"
                                            android:textColor="#000000"
                                            android:layout_gravity="center"
                                            android:layout_weight=".5"
                                            app:fontFamily="@font/architects_daughter"
                                            android:layout_height="0dp"
                                            android:text="Emergency">

                                        </TextView>

                                    </LinearLayout>
                                </androidx.cardview.widget.CardView>


                            </LinearLayout>

                        </LinearLayout>





























                    </LinearLayout>


            </ScrollView>











        </RelativeLayout>


</FrameLayout>